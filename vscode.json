{
    "terminal.integrated.rendererType": "experimentalWebgl",
    "editor.fontFamily": "Fira Code",
    "editor.fontWeight": "400",
    "editor.fontLigatures": true,
    "window.zoomLevel": 0,
    "breadcrumbs.enabled": false,
    "css.validate": false,
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.fontSize": 15,
    "editor.insertSpaces": true,
    "editor.letterSpacing": 0.5,
    "editor.lineHeight": 26,
    "editor.minimap.enabled": false,
    "editor.renderWhitespace": "all",
    "editor.suggest.localityBonus": true,
    "editor.wordWrap": "on",
    "emmet.showAbbreviationSuggestions": false,
    "eslint.enable": true,
    "eslint.run": "onSave",
    "eslint.validate": ["javascript", "javascriptreact"],
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "extensions.ignoreRecommendations": false,
    "files.autoSave": "onFocusChange",
    "files.trimTrailingWhitespace": true,
    "git.autofetch": true,
    "git.confirmSync": false,
    "git.enableSmartCommit": true,
    "html.format.indentHandlebars": true,
    "html.format.indentInnerHtml": true,
    "html.format.maxPreserveNewLines": 1,
    "html.format.preserveNewLines": true,
    "javascript.format.insertSpaceBeforeFunctionParenthesis": false,
    "javascript.updateImportsOnFileMove.enabled": "always",
    "languageStylus.useSeparator": true,
    "php.suggest.basic": true,
    "problems.decorations.enabled": true,
    "scss.validate": false,
    "search.smartCase": true,
    "search.useIgnoreFiles": true,
    "typescript.check.npmIsInstalled": false,
    "typescript.format.insertSpaceBeforeFunctionParenthesis": false,
    "workbench.colorTheme": "Panda Syntax",
    "workbench.iconTheme": "material-icon-theme",
    "workbench.startupEditor": "newUntitledFile",
    "emmet.includeLanguages": {
        "javascript": "html"
    },
    "files.associations": {
        ".stylelintrc": "jsonc",
        ".eslintrc": "jsonc",
        ".lintstagedrc": "json",
        ".huskyrc": "json",
        ".postcssrc": "json"
    },
    "eslint.options": {
        "extensions": [".js", ".vue", ".jsx", "ts", "tsx", ".json"]
    },
    "bracketPairColorizer.consecutivePairColors": [
        "[]",
        ["#aabff9", "#88a6f6", "#5680f3"],
        "Red"
    ],
    "workbench.settings.useSplitJSON": true,
    "editor.autoClosingBrackets": "beforeWhitespace",
    "[jsonc]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "window.titleBarStyle": "custom",
    "workbench.tree.renderIndentGuides": "none",
    "terminal.integrated.lineHeight": 1.2,
    "terminal.integrated.shell.linux": "/usr/bin/zsh",
    "emmet.triggerExpansionOnTab": true,
    "terminal.integrated.fontFamily": "\"Lucida Console\", Monaco, monospace",
    "stylusSupremacy.alwaysUseZeroWithoutUnit": true,
    "stylusSupremacy.insertBraces": false,
    "stylusSupremacy.insertSemicolons": false,
    "stylusSupremacy.quoteChar": "\"",
    "stylusSupremacy.reduceMarginAndPaddingValues": true,
    "stylusSupremacy.selectorSeparator": "\n",
    "[html]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[json]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "terminal.integrated.cursorBlinking": true,
    "npm.packageManager": "yarn",
    "typescript.updateImportsOnFileMove.enabled": "always",
    "[typescript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[typescriptreact]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "terminal.integrated.fontSize": 16,
    "breadcrumbs.icons": false,
    "breadcrumbs.filePath": "off",
    "breadcrumbs.symbolPath": "off",
    "javascript.preferences.quoteStyle": "single",
    "typescript.preferences.quoteStyle": "single",
    "editor.autoSurround": "languageDefined",
    "phpserver.browser": "firefox",
    "editor.maxTokenizationLineLength": 200000,
    "[stylus]": {
        "editor.defaultFormatter": "thisismanta.stylus-supremacy"
    },
    "[vue]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "workbench.activityBar.visible": true,
    "workbench.statusBar.visible": true,
    "codesnap.containerPadding": "3rem",
    "editor.semanticHighlighting.enabled": true,
    "codesnap.backgroundColor": "#f85888",
    "prettier.arrowParens": "always",
    "prettier.endOfLine": "lf",
    "prettier.singleQuote": true,
    "prettier.tabWidth": 4,
    "prettier.trailingComma": "es5",
    "editor.suggest.maxVisibleSuggestions": 5,
    "workbench.editor.limit.value": 3,
    "editor.detectIndentation": false,
    "workbench.editor.limit.enabled": true,
    "javascript.format.semicolons": "insert",
    "workbench.editor.limit.perEditorGroup": true,
    "workbench.sideBar.location": "left",
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
        "source.fixAll": true
    },
    "search.exclude": {
        "**/.git": true
    },
    "search.sortOrder": "modified",
    "search.useGlobalIgnoreFiles": true,
    "terminal.integrated.shell.osx": "/bin/zsh",
    "codesnap.boxShadow": "rgba(0, 0, 0, 0.55) 0px 5px 20px",
    "codesnap.transparentBackground": true,
    "prettier.disableLanguages": ["javascript", "javascriptreact"],
    "eslint.packageManager": "npm",
    "kite.showWelcomeNotificationOnStartup": false,
    "code-runner.clearPreviousOutput": true,
    "code-runner.saveAllFilesBeforeRun": true,
    "code-runner.showExecutionMessage": false,
    "workbench.list.smoothScrolling ": true,
    "[javascript]": {
        "editor.formatOnSave": false
    },
    "[javascriptreact]": {
        "editor.formatOnSave": false
    }
}
