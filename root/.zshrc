function 3bra(){
  if [ "$1" -eq "" ]; then
    git branch
    read test
    echo $test
  fi
  git checkout "$1$test"
  if [ $? -eq 0 ]; then
    npm i
    npm rebuild node-sass --force
    npm run start
  fi
}

